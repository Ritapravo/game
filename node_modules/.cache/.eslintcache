oin.id); return; }\n        \n        if (props.turn && props.enterEnabled)\n            setMoveOn(true);\n    }\n\n    const Coin_marker = {\n        margin: props.coin.margin,\n        backgroundColor: props.coin.backgroundColor,\n        borderRadius: '80% 80% 0% 80%',\n        transform: \"rotate(45deg)\",\n        border: '1px solid rgb(0, 0, 0)',\n        gridRowStart: x_val(counter),\n        gridColumnStart: y_val(counter),\n        // transition: \"all 2s ease-in\"\n    };\n    if(props.available[props.coin.id]===0){\n        Coin_marker.backgroundColor=\"grey\";\n        Coin_marker.opacity=\"80%\";\n    }\n\n\n    return (\n        <div\n            style={Coin_marker}\n            className={props.turn && props.enterEnabled ? \"blinker\" : \"\"}\n            onClick={slide}\n        >\n            <div className=\"circle\"/>\n        </div>\n    );\n}\nexport default Coin;","/home/ritapravo/Desktop/r-app/src/functions.js",[],[],"/home/ritapravo/Desktop/git/game/src/index.js",[],[],"/home/ritapravo/Desktop/git/game/src/reportWebVitals.js",[],[],"/home/ritapravo/Desktop/git/game/src/App.js",[],[],"/home/ritapravo/Desktop/git/game/src/functions.js",[],[],"/home/ritapravo/Desktop/git/game/src/components/Board.js",[],[],"/home/ritapravo/Desktop/git/game/src/components/Intro.js",[],[],"/home/ritapravo/Desktop/git/game/src/components/Button.js",[],[],"/home/ritapravo/Desktop/git/game/src/components/Coin.js",["85"],[],{"ruleId":"86","severity":1,"message":"87","line":28,"column":8,"nodeType":"88","endLine":28,"endColumn":16,"suggestions":"89"},{"ruleId":"86","severity":1,"message":"87","line":28,"column":8,"nodeType":"88","endLine":28,"endColumn":16,"suggestions":"90"},"react-hooks/exhaustive-deps","React Hook React.useEffect has missing dependencies: 'props' and 'speed'. Either include them or remove the dependency array. However, 'props' will change when *any* prop changes, so the preferred fix is to destructure the 'props' object outside of the useEffect call